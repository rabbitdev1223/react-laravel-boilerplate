{
	"presets": ["react", "es2015"],
	"plugins": [
		"transform-object-rest-spread",
		"transform-es2015-destructuring",
		["react-css-modules", {
		  "filetypes": {
		    ".scss": {
		      "syntax": "postcss-scss",
		      "plugins": ["postcss-nested"]
		    }
		  },
		  "generateScopedName": "[local]_[hash:base64:5]"
		}],
		"react-hot-loader/babel"
	]
}
